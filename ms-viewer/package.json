    "main": "dist/index.js",
    "directories": {
        "dist": "dist",
        "tests": "tests"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "jest --passWithNoTests --detectOpenHandles --coverage --testTimeout=900000 --runInBand",
        "start:client": "webpack-dev-server --config ./webpack.config.client",
        "start:server": "tsnd --respawn src/server/index.ts",
        "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
        "start-prod": "cd dist/server &&  node index.js",
        "build": "npm run clean:outdir && webpack --mode production --config webpack.config.js && webpack --mode production --config webpack.config.client.js",
        "build-client": "npm run clean:outdir && webpack --mode production --config webpack.config.client.js",
        "build-server": "npm run clean:outdir && webpack --mode production --config webpack.config.js",
        "bundle": "npx rimraf dist && tsc -d false",
        "lint": "eslint --ext \".ts,.js\" src --quiet",
        "clean": "npx rimraf dist",
        "clean:outdir": "npx rimraf __test__ dist",
        "format": "prettier --check \"src/**/*.ts\" --write"
    },
